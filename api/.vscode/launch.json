  "version": "0.2.0",
  "configurations": [
    {
      "request": "launch",
      "name": "Launch Program",
      "type": "pwa-node",
      "program": "${workspaceFolder}/test/mod.test.ts DENO_ENV=test",
      "cwd": "${workspaceFolder}",
      "runtimeExecutable": "deno",
      "runtimeArgs": [
        "test",
        "--allow-all",
        "--import-map=import_map.json",
        "--unstable",
        "--filter"
      ],
      "attachSimplePort": 9229
    }
  ]
}
